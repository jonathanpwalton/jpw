{
	"name": "wget",
	"latest": "2.2.0",
	"recipes": {
		"2.2.0": {
			"script": [
				"set -e",
				"sudo mkdir -p /var/cache/jpw",
				"cd /var/cache/jpw",
				"sudo wget -q --show-progress http://ftpmirror.gnu.org/wget/wget2-2.2.0.tar.gz",
				"sudo tar xzf wget2-2.2.0.tar.gz",
				"cd wget2-2.2.0",
				"./configure",
				"sudo make",
				"sudo make install"
			]
		}
	}
}